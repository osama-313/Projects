    <display-name>FindAccountByUser</display-name>
    <servlet-name>FindAccountByUser</servlet-name>
    <servlet-class>com.BankAPI.operations.FindAccountByUser</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>FindAccountByUser</servlet-name>
    <url-pattern>/accounts/owner/*</url-pattern>
  </servlet-mapping>
</web-app>